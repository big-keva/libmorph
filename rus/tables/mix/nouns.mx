// Беглые гласные в существительных мужского рода.
// В именительном падеже в словах, помеченных звездочкой, употребляется первая
// из перечисленных в строке последовательностей (у неодушевленных также и в
// винительном). В остальных падежах, также как и во множественном числе,
// употребляется вторая из них.

// о             - так называемый "обычный" случай
// ле            - перед последней согласной стоит "ле"
// ге            - перед последней согласной стоит гласная + е

.таблица м 1*a, мо 1*a, м 1*a1
{
  ге ем йм              // за|ем| - за|йм|а
  о  ет т               // егип|ет| - егип|т|а
  о  ор р               // свек|ор| - свек|р|а
  о  он н               // коз|он| - коз|н|а
  о  ен н               // буб|ен| - буб|н|а
  о  ел л               // пеп|ел| - пеп|л|а
}

.таблица м 1*b, м 1*b2, м 1*b3, мо 1*b, м//мо 1*b, мо//м 1*b
{
  ле ед ьд              // л|ед| - л|ьд|а
  ле ев ьв              // л|ев| - л|ьв|а
  ле ен ьн              // л|ен| - л|ьн|а
  о  от т               // р|от| - р|т|а
  о  ор р               // буг|ор| - буг|р|а
  о  он н               // рож|он| - рож|н|а
  о  ом м               // псал|ом| - псал|м|а
  о  ол л               // уг|ол| - уг|л|а
  о  ов в               // р|ов| - р|в|а
  о  об б               // л|об| - л|б|а
  о  ес с               // ов|ес| - ов|с|а
  о  ер р               // ков|ер| - ков|р|а
  о  ел л               // кот|ел| - кот|л|а
  о  ет т               // хреб|ет| - хреб|т|а
  о  ан н               // коч|ан| - коч|н|а
  о  ен н               // ов|ен| - ов|н|а
}

.таблица м 1*e, мо 1*e, м 1*e2//1*a2
{
  о  ер р               // вет|ер| - вет|р|а
}

.таблица м 2*a, м 2*a1, мо 2*a, м 2*a//2*e, м 2*e//2*a
{
  ле ен ьн              // увал|ен|ь - увал|ьн|я
  о  от т               // дег|от|ь - дег|т|я
  о  ен н               // лив|ен|ь - лив|н|я
  о  ел л               // стеб|ел|ь - стеб|л|я
  о  ол л               // уг|ол|ь - уг|л|я
}

.таблица м 2*b, мо 2*b
{
  о  он н               // ог|он|ь - ог|н|я
  о  от т               // лом|от|ь - лом|т|я
  о  ор р               // уг|ор|ь - уг|р|я
  о  ен н               // рем|ен|ь - рем|н|я
  о  ел л               // журав|ел|ь - журав|л|я
}

.таблица м 2*e, мо 2*e
{
  о  от т               // ног|от|ь - ног|т|я
  о  ен н               // кор|ен|ь - кор|н|я
}

.таблица м 3*a, м 3*a-, м 3*a1, мо 3*a, м 3*a"1", мо 3*a"1"
{
  ге ек йк              // переше|ек| - переше|йк|а
  о  ок к               // куб|ок| - куб|к|а
  о  ек к               // кусоч|ек| - кусоч|к|а
}

.таблица м 3*b, м 3*b1, м 3*b2, мо 3*b, м 3*b"1", мо 3*b"1", м 3*d"1", мо 3*d"1", м 3*d"2", мо 3*d"2", м//мо 3*b, м 3*b:m2, м 3*b!
{
  ге ек йк              // па|ек| - па|йк|и
  о  ок к               // кус|ок| - кус|к|и
  о  ек ьк              // хор|ек| - хор|ьк|и
  о  ох х               // м|ох| - м|х|и
}

.таблица м 5*a, м 5*a-, мо 5*a, м 5*a1, м 5*a:m5
{
  ле ец ьц              // пал|ец| - пал|ьц|ы
  ге ец йц              // кита|ец| - кита|йц|ы
  о  ец ц               // нем|ец| - нем|ц|а
  о  яц йц              // за|яц| - за|йц|ы
}

.таблица м 5*b, м 5*b-, м 5*b1, мо 5*b
{
  ле ец ьц              // жил|ец| - жил|ьц|ы
  ге ец йц              // бо|ец| - бо|йц|ы
  о  ец ц               // кон|ец| - кон|ц|ы
}

.таблица мо E1
{
  о  ин н               // од|ин| - од|н|и
}

