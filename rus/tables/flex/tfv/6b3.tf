  .лицо 2
    е,,"9"              // сошл-е-шь(ся), сошл-е-те(сь)
  .лицо 3
    '',,"10"            // сошл-ет(ся), сошл-ют(ся)
.время П        // прошедшее время
    ал,,"3"             // сосл-ал(-(ся),-а(сь),-о(сь),-и(сь))
  .действ
    авш,,"5"            // сосл-авш-ий(ся)
  .страд
    ан,,"6"             // сосл-ан-ный
